{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.json": [
      "@parcel/transformer-raw"
    ]
  },
  "optimizers": {
    "*.js": [
      "@parcel/optimizer-esbuild"
    ]
  },
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  },
  "reporters": [
    "...",
    "parcel-reporter-static-files-copy"
  ]
}
